#!/bin/bash

PYTHON=/usr/bin/python
EPID='EPID-1001'
CE_HOST=11.126.32.9		# Tsc Server
#CE_HOST=11.126.32.12	# Dan Ubuntu
#CE_HOST=127.0.1.1		# Virtual Machine
CE_PORT=8000

if [ -n "$TWISTER_PATH" ]
then
	echo TWISTER_PATH set to: "$TWISTER_PATH".
	$PYTHON -u $TWISTER_PATH/client/executionprocess/ExecutionProcess.py $EPID "$CE_HOST:$CE_PORT" | tee $TWISTER_PATH/.twister_cache/$EPID"_LIVE.log"
else
	echo TWISTER_PATH variable enviroment is not defined!
fi
